{
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "es6",
    "module": "es6",
    "esModuleInterop": true,
    "outDir": "wwwroot/scripts/"
  },
  "exclude": [
    "node_modules",
     "wwwroot/assets/vendor"
  ],
  "include": [
    "wwwroot/scripts/"
  ]
}
